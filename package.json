{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build && node scripts/fix-client-manifests.mjs",
    "build:quiet": "cross-env NODE_OPTIONS=--no-deprecation next build && node scripts/fix-client-manifests.mjs",
    "db:setup": "tsx scripts/setup-database.ts",
    "db:setup:script": "tsx scripts/setup-database.ts --script",
  "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "lint": "next lint",
    "load:test": "k6 run tests/load/auth-load-test.js",
  "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:security": "npm run test:security:owasp && npm run test:security:deps",
    "test:security:deps": "npm audit --audit-level=moderate",
    "test:security:owasp": "echo 'Run OWASP ZAP scan manually'",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
